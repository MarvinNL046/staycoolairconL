{
  "name": "staycoolairconl",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:brands && npm run optimize-images && npm run generate-sitemaps && npm run build:clean && npm run build:client && npm run validate:build",
    "build:clean": "rimraf dist",
    "build:client": "tsc && vite build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:brands": "node scripts/build-brands-data.js && node scripts/bundle-brands-data.js",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-sitemaps": "node scripts/generate-sitemaps-unified.js",
    "generate-sitemaps-old": "node scripts/generate-products-sitemap.js && node scripts/generate-service-areas-sitemap.js && node scripts/generate-sitemap.js && node scripts/update-sitemap-with-locations.js && node scripts/generate-blog-sitemap.js",
    "optimize-images": "node scripts/optimize-product-images.js",
    "analyze-bundle": "npx tsx src/scripts/analyzeBundleSize.ts",
    "analyze-schema": "npx tsx src/scripts/runSchemaAudit.ts",
    "performance-audit": "npm run analyze-bundle && npm run analyze-schema",
    "validate:build": "node scripts/validate-build.cjs",
    "cache:clear": "node scripts/clear-cache.cjs"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@stripe/react-stripe-js": "^3.1.1",
    "@stripe/stripe-js": "^5.6.0",
    "@supabase/supabase-js": "^2.49.3",
    "@types/jspdf": "^2.0.0",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "jspdf": "^2.5.2",
    "papaparse": "^5.4.1",
    "playwright": "^1.54.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-feather": "^2.0.10",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.5.1",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^6.22.0",
    "web-vitals": "^4.2.4",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.11.17",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "framer-motion": "^12.4.2",
    "lucide-react": "^0.475.0",
    "postcss": "^8.4.35",
    "rimraf": "^5.0.10",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
